{
    "name": "laos-package-manager",
    "version": "1.0.0",
    "description": "Simple Ubuntu Package Manager by LAOS",
    "main": "app/main.js",
    "scripts": {
        "start": "electron .",
        "package-linux": "electron-packager . LaosPackageManager --overwrite --asar --platform=linux --arch=x64 --icon=Web/img/logo.png --prune=true --out=release-builds --ignore=node_modules",
        "create-debian-installer": "electron-installer-debian --src release-builds/laos-package-manager-x64/ --arch amd64 --config debian.json"
    },
    "repository": "",
    "keywords": [
        "Electron",
        "package",
        "manager",
        "debian"
    ],
    "author": "evaleries",
    "license": "MIT",
    "devDependencies": {
        "electron": "^6.0.8",
        "jquery": "^3.4.1"
    },
    "dependencies": {
        "angular": "^1.7.8",
        "angular-route": "^1.7.8",
        "electron-download-manager": "^2.1.2",
        "electron-installer-debian": "^2.0.1",
        "electron-store": "^5.0.0",
        "md5-file": "^4.0.0",
        "slugify": "^1.3.5",
        "sudo-prompt": "^9.0.0"
    }
}
