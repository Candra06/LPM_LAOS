{
    "name": "laos-package-manager",
    "productName": "Laos Package Manager",
    "version": "1.0.0",
    "description": "Simple Ubuntu Package Manager by LAOS",
    "main": "app/main.js",
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "laos-package-manager",
        "files": [
            "**/*"
        ],
        "linux": {
            "target": [
                "deb"
            ],
            "maintainer": "evaleries",
            "artifactName": "${productName}-${version}-${arch}.${ext}"
        },
        "deb": {
            "synopsis": "Laos Package Manager"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Candra06/LPM_LAOS.git"
    },
    "bugs": {
        "url": "https://github.com/Candra06/LPM_LAOS/issues"
    },
    "keywords": [
        "Electron",
        "package",
        "manager",
        "debian"
    ],
    "author": "evaleries",
    "license": "MIT",
    "devDependencies": {
        "electron": "^6.0.8",
        "electron-builder": "^22.1.0"
    },
    "dependencies": {
        "jquery": "^3.4.1",
        "angular": "^1.7.8",
        "angular-route": "^1.7.8",
        "electron-download-manager": "^2.1.2",
        "electron-store": "^5.0.0",
        "md5-file": "^4.0.0",
        "slugify": "^1.3.5",
        "sudo-prompt": "^9.0.0"
    }
}
